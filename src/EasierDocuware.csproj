<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<PropertyGroup>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NoWarn>$(NoWarn);1591</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="DocuWare.Platform.ServerClient" Version="14.11.1" />
		<PackageReference Include="DocuWare.Platform.ServerClient.Extensions" Version="12.11.0" />
		<PackageReference Include="DocuWare.WebIntegration" Version="10.2.1" />
		<PackageReference Include="Duende.IdentityModel" Version="7.1.0" />
		<PackageReference Include="Duende.IdentityModel.OidcClient" Version="6.0.1" />
	</ItemGroup>

</Project>
